{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adflab-adf300"
		},
		"AzStorage_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzStorage_Staging'"
		},
		"SSISDB Azure Endpoint_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SSISDB Azure Endpoint'"
		},
		"Az_blob_S3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Az_blob_S3'"
		},
		"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "adflabsqlzf6ow.database.windows.net"
		},
		"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "labadmin"
		},
		"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"S3_MasterFAAFile_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "FAAmaster.txt"
		},
		"S3_MasterFAAFile_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "blobsource/S3/S3"
		},
		"Master_FAAFile_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "FAAmaster.txt"
		},
		"Master_FAAFile_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "input/FAAmaster"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzStorage_Staging_connectionString": {
						"value": "[parameters('AzStorage_Staging_connectionString')]"
					},
					"SSISDB Azure Endpoint_connectionString": {
						"value": "[parameters('SSISDB Azure Endpoint_connectionString')]"
					},
					"Az_blob_S3_connectionString": {
						"value": "[parameters('Az_blob_S3_connectionString')]"
					},
					"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"S3_MasterFAAFile_properties_typeProperties_fileName": {
						"value": "[parameters('S3_MasterFAAFile_properties_typeProperties_fileName')]"
					},
					"S3_MasterFAAFile_properties_typeProperties_folderPath": {
						"value": "[parameters('S3_MasterFAAFile_properties_typeProperties_folderPath')]"
					},
					"Master_FAAFile_properties_typeProperties_fileName": {
						"value": "[parameters('Master_FAAFile_properties_typeProperties_fileName')]"
					},
					"Master_FAAFile_properties_typeProperties_folderPath": {
						"value": "[parameters('Master_FAAFile_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}